    "version": "0.2.0",
    "configurations": [
        {
            "name": "feed readers",
            "type": "node",
            "request": "launch",
            "args": [
                "./feed-readers/index.ts"
            ],
            "runtimeArgs": [
                "-r",
                "ts-node/register"
            ],
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
            "internalConsoleOptions": "openOnSessionStart",
            "env": {
                "FEEDS_TO_READ": "https://elmundo.es,https://elpais.com"
            }
        }
    ]
}